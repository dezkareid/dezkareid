{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "outputs": [],
      "inputs": ["src/**/*.jsx", "src/**/*.js"]
    },
    "lint": {
      "outputs": []
    },
    "release": {
      "dependsOn": ["build"],
      "outputs": []
    }
  }
}
