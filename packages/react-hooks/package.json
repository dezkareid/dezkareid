{
  "name": "@dezkareid/react-hooks",
  "version": "0.0.0-semantic-release",
  "description": "set of react hooks with common functionalities",
  "keywords": [
    "react",
    "hooks"
  ],
  "sideEffects": false,
  "author": "Joel Humberto Gomez Paredes <elmaildeldezkareid@gmail.com>",
  "license": "MIT",
  "unpkg": "dist/browser",
  "main": "dist/cjs",
  "module": "dist/es",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run build:rollup && pnpm run build:types && pnpm run build:webpack",
    "build:rollup": "rollup -c --config-file rollup.config.js --environment BUILD_ES_CJS",
    "build:webpack": "webpack --config webpack/webpack.config.cjs",
    "build:types": "rollup -c --config-file rollup.config.js --environment BUILD_TYPES",
    "start": "vite",
    "lint": "eslint -c eslint.config.mjs .",
    "lint:fix": "npm run lint -- --fix",
    "semantic-release": "semantic-release",
    "release": "semantic-release",
    "test": "vitest --run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dezkareid/dezkareid.git"
  },
  "bugs": {
    "url": "https://github.com/dezkareid/dezkareid/issues"
  },
  "engines": {
    "node": ">=22.17"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.28.5",
    "@eslint/js": "9.13.0",
    "@rollup/plugin-babel": "6.1.0",
    "@rollup/plugin-commonjs": "29.0.0",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "16.0.3",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "12.3.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@types/jest": "30.0.0",
    "@types/node": "25.0.10",
    "@types/react": "19.2.9",
    "@typescript-eslint/eslint-plugin": "8.54.0",
    "@typescript-eslint/parser": "8.54.0",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/ui": "4.0.18",
    "camel-case": "5.0.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "globals": "15.11.0",
    "jsdom": "27.4.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.8.1",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-test-renderer": "19.2.4",
    "rollup": "4.56.0",
    "rollup-plugin-bundle-size": "1.0.3",
    "rollup-plugin-cleaner": "1.0.0",
    "rollup-plugin-dts": "6.3.0",
    "semantic-release": "25.0.2",
    "semantic-release-monorepo": "8.0.2",
    "ts-loader": "9.5.4",
    "typescript": "5.9.3",
    "vitest": "4.0.18",
    "vitest-dom": "0.1.1",
    "webpack": "5.104.1",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.3",
    "webpack-merge": "6.0.1"
  }
}