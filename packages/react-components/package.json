{
  "name": "@dezkareid/react-components",
  "version": "0.0.0-semantic-release",
  "description": "set of components with common functionalities",
  "keywords": [
    "react",
    "components"
  ],
  "sideEffects": false,
  "author": "Joel Humberto Gomez Paredes <elmaildeldezkareid@gmail.com>",
  "license": "MIT",
  "unpkg": "dist/browser",
  "main": "dist/cjs",
  "module": "dist/es",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:rollup && npm run build:webpack",
    "build:rollup": "rollup -c",
    "build:webpack": "webpack --config webpack/webpack.config.js",
    "start": "webpack serve --config webpack/webpack.dev.js",
    "lint": "eslint ./src",
    "lint:fix": "npm run lint -- --fix",
    "semantic-release": "semantic-release",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dezkareid/dezkareid.git"
  },
  "bugs": {
    "url": "https://github.com/dezkareid/dezkareid/issues"
  },
  "engines": {
    "node": ">=14.17"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^16.0.0 || ^17.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-export-default-from": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-react": "^7.14.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@testing-library/dom": "^8.7.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.1",
    "@testing-library/react-hooks": "^7.0.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.2.4",
    "babel-loader": "^8.2.2",
    "camel-case": "^4.1.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.2.4",
    "prettier": "^2.4.1",
    "prop-types": "^15.7.2",
